{
  "name": "parcel-plugin-wx-miniapp",
  "version": "0.0.8",
  "description": "Parcel Plugin for Wechat MiniApp",
  "main": "index.js",
  "repository": "git@github.com:Jimexist/parcel-plugin-wx-miniapp.git",
  "author": "Jiayu Liu <etareduce@gmail.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "test": "cross-env NODE_ENV=test jest",
    "clean": "rimraf lib/",
    "prepublishOnly": "npm run test",
    "pretest": "npm run lint",
    "lint": "eslint src",
    "format": "prettier --write \"./{src,test}/**/*.{js,json,md}\""
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+.js$": "babel-jest"
    }
  },
  "peerDependencies": {
    "parcel-bundler": "^1.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.52",
    "babel-eslint": "^8.2.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.3.0",
    "lint-staged": "^7.2.0",
    "parcel-assert-bundle-tree": "^1.0.0",
    "parcel-bundler": "^1.7.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
